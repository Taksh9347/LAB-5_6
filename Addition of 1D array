#include<stdio.h>
int main()
{
	int i,a[10],b[10],c[10];
	printf("Enter 10 elements for first array \n");
	for(i=0;i<10;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Enter 10 elements for second array \n");
	for(i=0;i<10;i++)
	{
		scanf("%d",&b[i]);
	}
	for(i=0;i<10;i++)
	{
		c[i]=a[i]+b[i];
	}
	for(i=0;i<10;i++)
	{
		printf("%d ",c[i]);
	}
}
