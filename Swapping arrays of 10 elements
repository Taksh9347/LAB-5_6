#include<stdio.h>
int main()
{
	int i,a[10],b[10],c[10];
	printf("Enter 10 elements for first array \n");
	for(i=0;i<10;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Enter 10 elements for second array \n");
	for(i=0;i<10;i++)
	{
		scanf("%d",&b[i]);
	}
	for(i=0;i<10;i++)
	{
		c[i]=a[i];
		a[i]=b[i];
		b[i]=c[i];
	}
	printf("First array after swapping is \n");
	for(i=0;i<10;i++)
	{
		printf("%d ",a[i]);
	}
	printf("\n");
	printf("Second array after swapping is \n");
	for(i=0;i<10;i++)
	{
		printf("%d ",b[i]);
	}
}
