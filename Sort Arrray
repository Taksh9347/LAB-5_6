#include<stdio.h>
int main()
{
	int i,a[10],j,x;
	printf("Enter 10 elements for array \n");
	for(i=1;i<=10;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=1;i<=10;i++)
	{
		for(j=i+1;j<=10;j++)
		{
			if(a[i]>a[j])
			{
				x=a[i];
				a[i]=a[j];
				a[j]=x;
			}
		}
	}
	printf("Array after sorting is \n");
	for(i=1;i<=10;i++)
	{
		printf("%d ",a[i]);
	}}
