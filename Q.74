#include<stdio.h>
int main()
{
	int index=0,x,i,j,a[]={1,2,8,14,50},b[]={6,7,11,12,15},c[10];
	for(i=0;i<5;i++)
	{
		c[index++]=a[i];
	}
	for(i=0;i<5;i++)
	{
		c[index++]=b[i];
	}
	printf("Array C before sorting is \n");
	for(i=0;i<10;i++)
	{
		printf("%d ",c[i]);
	}
		for(i=0;i<10;i++)
	{
		for(j=i+1;j<10;j++)
		{
			if(c[i]>c[j])
			{
				x=c[i];
				c[i]=c[j];
				c[j]=x;
			}
		}
}
printf("\n");
printf("Array C in ascending order is \n");
	for(i=0;i<10;i++)
	{
		printf("%d ",c[i]);
	}}
