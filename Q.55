#include<stdio.h>
int main()
{
	int i,o,e,a[10];
	printf("Enter 10 elements for the array \n");
	for(i=1;i<=10;i++)
	{
		scanf("%d",&a[i]);
	}
	o=0,e=0;
	for(i=1;i<=10;i++)
	{
		if(a[i]%2==0)
		{
			e++;
		}
		else
		{
			o++;
		}
	}
	printf("%d are odd numbers\n",o);
	printf("%d are even numbers",e);
}
