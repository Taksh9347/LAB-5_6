#include<stdio.h>
int main()
{
	int t,i,j,a[3][3],b[3][3];
	printf("Enter elements for a 3*3 matrix \n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	printf("Matrix is\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
		     printf(" %d ",a[i][j]);
		}
		printf("\n");
	}
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			b[i][j]=a[j][i];
		}	
	}

	printf("Transpose is \n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
		     printf(" %d ",b[i][j]);
		}
		printf("\n");
	}
}
